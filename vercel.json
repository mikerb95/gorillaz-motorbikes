{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "favicons/**", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/public/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/favicons/(.*)", "dest": "/favicons/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/images/(.*)", "dest": "/images/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
